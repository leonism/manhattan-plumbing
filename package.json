    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-html-minifier": "^1.0.5",
    "vite-plugin-html-minify": "^1.0.0",
    "vite-plugin-image-optimizer": "^2.0.2",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-webp-generator": "^0.1.0"
  }
