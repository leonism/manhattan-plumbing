{
  "name": "vite-react-typescript-manhattan-plumbing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite && npm run generate",
    "build": "vite build && npm run generate && npm run prerender:pages",
    "lint:fix": "eslint . --ext css,.js,.jsx,.ts,.tsx --fix && prettier --write .",
    "format": "prettier --write \"*.{css,js,ts,jsx,tsx,md}\" \"src/**/*.{js,ts,jsx,tsx,md}\" \"src/components/**/*.{js,ts,jsx,tsx,md}\" --ignore-path .prettierignore",
    "check:code": "npm run format && npm run lint:fix",
    "preview": "vite preview",
    "serve:dist": "serve dist",
    "generate": "npm run generate:sitemap && npm run generate:rss",
    "prerender:pages": "node scripts/prerender-pages.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:rss": "node scripts/generate-rss.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@mdx-js/react": "^3.0.0",
    "@mdx-js/rollup": "^3.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react-helmet": "^6.1.11",
    "@vercel/analytics": "^1.5.0",
    "buffer": "^6.0.3",
    "cheerio": "^1.1.0",
    "date-fns": "^3.3.1",
    "front-matter": "^4.0.2",
    "gray-matter": "^4.0.3",
    "iltorb": "^2.4.5",
    "lucide-react": "^0.344.0",
    "marked": "^16.0.0",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.2",
    "react-slick": "^0.30.3",
    "rehype-raw": "^7.0.0",
    "rehype-react": "^8.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^4.0.0",
    "remark-rehype": "^11.1.2",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "feed": "^4.2.2",
    "fs-extra": "^11.3.0",
    "globals": "^15.9.0",
    "globby": "^14.1.0",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webp": "^8.0.0",
    "lightningcss": "^1.30.1",
    "postcss": "^8.4.35",
    "serve": "^14.2.4",
    "sharp": "^0.34.2",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.11",
    "terser": "^5.39.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.3.2",
